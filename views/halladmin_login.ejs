<%- include('partials/header') %>

<div class="signupform">
  <form action="/halladmin/login/<%= hall %>" method="post">
    <a href="/student/login" class="signuplinks">Student</a>
    <a href="/halladmin/login" class="signuplinks">Hall Admin</a><br />
    <p>Hall Admin Login</p>
    <div>
      <label for="staffid">Staff Id</label><br />
      <input type="text" placeholder="Staff Id" name="username" /><br />
    </div>
    <div>
      <label for="pasword">Password</label><br />
      <input type="password" name="password" placeholder="Password" /><br />
    </div>
    <button type="submit">Login</button>
    <p>Don't have an account? <a href="/halladmin/signup">Sign up</a> here</p>
  </form>
</div>
