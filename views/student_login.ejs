<%- include('partials/header') %>

<div class="signupform">
  <form action="/student/login" method="post">
    <a href="/student/login" class="signuplinks">Student</a>
    <a href="/halls" class="signuplinks">Hall Admin</a><br />
    <p>Login</p>
    <div>
      <label for="matno">Matric Number</label><br />
      <input type="text" placeholder="Matric Number" name="username" /><br />
    </div>
    <div>
      <label for="pasword">Password</label><br />
      <input type="password" name="password" placeholder="Password" /><br />
    </div>
    <button type="submit">Login</button>
    <p>Don't have an account? <a href="/student/signup">Sign up</a> here</p>
  </form>
</div>
